
.header{
  display: flex;
  margin: 20rpx 0;
}
.address{
  width: 158rpx;
  font-size: 28rpx;
}
.address image{
  width: 22rpx;
  height: 28rpx;
  margin-left: 40rpx;
  margin-right: 12rpx;
  padding-top: 15rpx;
}
.search{
  position: relative;
}
.search icon{
  position: absolute;
  left: 26rpx;
  top: 15rpx;
  z-index: 2;
}
.search input{
  width: 500rpx;
  border:1rpx solid #bfbfbf;
  margin-right: 20rpx;
  border-radius: 30rpx;
  padding: 5rpx 0rpx;
  font-size: 28rpx;
  text-align: left;
  color: #b2b2b2;
  padding-left: 60rpx;
}
.header text{
  font-size: 28rpx;
  margin-top: 13rpx;
}


.slides {
	height: 580rpx;
}

.slides navigator{
  height: 100%;
}
.slides image {
	width: 100%;
	height: 100%;
}

.grids{
  display: flex;
  justify-content: space-around;
  padding-bottom: 30rpx;
  padding-left: 10rpx;
  padding-right: 10rpx;
  padding-top: 20rpx;
}
.grids .item{
  display: flex;
  flex-direction: column;
  width: 20%;
  justify-content: center;
	align-items: center;
  margin-top: 30rpx;
  box-sizing: border-box;
}
.grids .item image{
  width: 87rpx;
  height: 87rpx;
}
.grids .item text{
  font-size: 24rpx;
  margin-top: 20rpx;
  color: #333333;
}
.cells{
  display: flex;
  flex-direction: column;
  position: relative;
}

.cells .bg{
  width: 300rpx;
  height: 44rpx;
  border-bottom: 2rpx solid #000;
  position: absolute;
  top: 0;
  left: 50%;
  transform: translateX(-50%);
}
.cells .title{
  font-size: 36rpx;
  line-height: 88rpx;
  height: 88rpx;
  width: 434rpx;
  text-align: center;
  margin: 0 auto;
  border-radius:44rpx;
  background-color: #f5f5f5;
  z-index: 999;
  color: #333333;
  position:relative;
}
.cells .title_1{
 position:relative;
 z-index:10;
 background:#f5f5f5;
 padding:0 20rpx;
}
.cells .pro_box{
  width: 100%;
  display: flex;
  flex-wrap: wrap;
  padding: 3%;
  box-sizing: border-box;
}
.scroll-view_H{
  display: flex;
  white-space: nowrap;
}
.cells .pro_box .box{
  margin: 2% 2%;
  width: 40%;
  display: inline-block;
  background-color: white;
  border-radius: 15rpx;
}
.cells .pro_box .box image{
  width: 100%;
  border-top-left-radius: 15rpx;
  border-top-right-radius: 15rpx;
  height: 266rpx;
}
.cells .pro_box .box text{
  font-size: 24rpx;
}
.cells .pro_box .box > text{
  font-size: 26rpx;
  padding: 10rpx;
  overflow:hidden;
  text-overflow:ellipsis;
  white-space:nowrap;
}
.cells .pro_box .box .info{
  padding: 0 10rpx;
  display: flex;
}
.cells .pro_box .box .info .tag{
  color: #ee2d4a;
  border: 1px solid #f32846;
  padding: 2rpx 6rpx;
  border-radius: 10rpx;
  font-size: 22rpx;
  margin-right: 8rpx;
  width: 66rpx;
  overflow:hidden;
  text-overflow:ellipsis;
  white-space:nowrap;
}
.cells .pro_box .box .info text:last-of-type{
  padding-top: 3rpx;
  color: #8d8352;
  overflow:hidden;
  text-overflow:ellipsis;
  white-space:nowrap;
  width: 70%;
}
.cells .pro_box .box .price{
  display: flex;
  justify-content: space-between;
  padding: 6rpx;
}
.cells .pro_box .box .price text{
  color: #f52322;
}
.cells .pro_box .box .price text:first-of-type text{
  font-size: 32rpx;
  font-weight: bold;
}
.cells .pro_box .box .price > .last{
  border: 1px solid #f52322;
  padding: 0rpx 10rpx;
  background-color: #f52322;
  border-radius: 30rpx;
  line-height: 39rpx;
  color: white;
}

.loadmore{
  background-color: #f5f5f5;
  font-size: 30rpx;
  line-height: 80rpx;
  text-align: center;
}
