.enroll {
  padding: 20rpx;
}

.enroll_item {
  border: 1px solid #ccc;
  border-radius: 10rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  margin: 20rpx 0;
  padding: 10rpx;
  box-shadow: 0 0 10rpx #ccc;
}

.item_title {
  display: flex;
  font-size: 40rpx;
  justify-content: space-between;
}

.title_name::before {
  content: '';
  width: 10rpx;
  height: 100%;
  border: 2px solid #ff534a;
  margin-right: 10rpx;
}

.title_name {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.title_tips {
  border: 2px solid #ff534a;
  padding: 0 5rpx;
  color: #ff534a;
  border-radius: 8rpx;
}

.item_title .item_end {
  border: 2px solid #666;
  color: #666;
}

.item_main {
  margin-top: 10rpx;
  background: #f5f5f5;
  font-size: #666;
  padding:10rpx;
}

.noinfo {
  text-align: center;
  font-size: 50rpx;
  margin-top: 30rpx;
}